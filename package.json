{
  "name": "trle-tropes",
  "version": "1.0.0",
  "description": "A list of helpful tropes in Tomb Raider custom levels, meant to be used as a reference for level building",
  "private": true,
  "scripts": {
    "build": "webpack",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "css-build": "node-sass scss/main.scss build/css/main.css --importer=node_modules/node-sass-tilde-importer",
    "css-watch": "npm run css-build -- --watch",
    "fix": "gts fix",
    "posttest": "npm.cmd run check",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "start": "webpack-dev-server --open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SidDays/trle-tropes.git"
  },
  "keywords": [
    "trle"
  ],
  "author": "Siddhesh Rajiv Karekar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SidDays/trle-tropes/issues"
  },
  "homepage": "https://github.com/SidDays/trle-tropes#readme",
  "devDependencies": {
    "@types/node": "^10.17.60",
    "@types/webpack": "^4.41.32",
    "@types/webpack-dev-server": "^3.11.6",
    "bootstrap": "^5.2.0",
    "gts": "^2.0.2",
    "node-sass": "^7.0.1",
    "node-sass-tilde-importer": "^1.0.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^4.10.0"
  }
}